{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular2-full-stack"
  },
  "apps": [
    {
      "root": "client",
      "outDir": "dist/public",
      "assets": [
        "assets",
        "assets/favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../client/assets/css/eddie.css",
        "../client/assets/css/bootstrap.min.css",
        "../client/assets/css/material-kit.css",
        "../client/assets/css/animate.css",
        "../client/assets/css/normalize.css",
        "../client/assets/css/ion.rangeSlider.css",
        "../client/assets/css/ion.rangeSlider.skinFlat.css",
        "styles.scss"
      ],
      "scripts": [
        "../client/assets/js/ali.js",
        "../client/assets/js/jquery.min.js",
        "../client/assets/js/bootstrap.min.js",
        "../client/assets/js/material.min.js",
        "../client/assets/js/bootstrap-datepicker.js",
        "../client/assets/js/material-kit.js",
        "../client/assets/js/ion.rangeSlider.js",
        "../client/assets/js/radarChart.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "client/tsconfig.app.json"
    },
    {
      "project": "client/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
